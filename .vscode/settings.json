{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": [
    "javascript",
    "typescript",
    "typescriptreact"
  ],
  "files.eol": "\n",
  "editor.tabSize": 2,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false
}